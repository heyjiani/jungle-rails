<section class="login-form">

  <header class="page-header">
    <h1>Log In</h1>
  </header>

  <div class="col-sm-5">
    <%= form_tag '/login' do %>

      Email: <%= text_field_tag :email %>
      Password: <%= password_field_tag :password %>
      <%= submit_tag "Submit" %>

    <% end %>
  </div>


</section>